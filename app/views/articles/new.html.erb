<h1>Create New Article</h1>

<% if @article.errors.any? %>
    <ul>
        <% @article.errors.full_messages.each do |message| %>
            <li> <%= message %></li>
        <%end%>
    </ul>
<% end %>
<%= render 'form' %>

<%= link_to "Back to listing", articles_path %>
<% content_for :javascript_includes do %>
  <%= javascript_include_tag "test.js" %>
<% end %>